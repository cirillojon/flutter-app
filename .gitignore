{
  "database": {
    "rules": "database.rules.json"
  },
  "hosting": {
    "public": "myapp/build/web",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}